# Jinja CPU Monitoring System - Configuration
# Created by Jinja (2025)

# Cloudflare API credentials
CF_API_KEY=your_cloudflare_api_key_here
CF_EMAIL=your_cloudflare_email_here
CF_ZONE_ID=your_cloudflare_zone_id_here

# CPU monitoring settings
CPU_THRESHOLD=80               # Percentage threshold to trigger attack mode
MONITORING_INTERVAL=10         # Time between checks in seconds
REQUIRED_HIGH_READINGS=3       # Number of consecutive high readings before enabling attack mode
REQUIRED_LOW_READINGS=5        # Number of consecutive low readings before disabling attack mode
